float Ax, Ay, Az, Gx, Gy, Gz;

uint8_t err_gyro = 0;

float CpmVoltage = 0.0;
float HVoltage = 0.0;
bool En_Voltage_ui=0;

float temperature;
uint16_t timer_val=0;

uint16_t dmaOut[4];

//Barometer_Sensor
float Temperature2;
float Pressure;
float Altitude;

//current_sensor 5A
uint16_t readValue5;
float rawVoltage5;
float current5;
float sensitivity5 = 0.185;
float adc_err5=1.052;

//current_sensor 20A
uint16_t readValue20;
float rawVoltage20;
float current20;
float sensitivity20 = 0.1;
float adc_err20=1.052;

//GPS Data
char GGA[100];
char RMC[100];

GPSSTRUCT gpsData;

//Nose Cone Pressure
uint16_t NCPressure=0;

char TxBuff[150];

